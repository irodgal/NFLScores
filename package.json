{
  "name": "nfl-scores",
  "version": "0.0.1",
  "description": "Código para la parte servidor",
  "main": "src/server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/irodgal/NFLScores"
  },
  "dependencies": {
    "express": "^4.13.3",
    "method-override": "^2.3.5",
    "request": "^2.67.0",
