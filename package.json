  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.6",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.4",
    "mocha-lcov-reporter": "^1.1.0",
    "supertest": "^1.2.0"
  },
  "scripts": {
